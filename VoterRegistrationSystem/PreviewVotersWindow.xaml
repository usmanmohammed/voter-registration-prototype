<Window x:Class="VoterRegistrationSystem.PreviewVotersWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VoterRegistrationSystem"
        mc:Ignorable="d"
        Title="PreviewVotersWindow" Height="700" Width="1200" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="415*"/>
            <RowDefinition Height="54*"/>
        </Grid.RowDefinitions>
        <Button x:Name="btnPrint" Content="Print" HorizontalAlignment="Right" Margin="0,0,10,0" Grid.Row="1" VerticalAlignment="Top" Width="154" Height="44" Click="button_Click"/>
        <WebBrowser x:Name="browser" HorizontalAlignment="Stretch" Margin="10" VerticalAlignment="Stretch"/>
    </Grid>
</Window>
